# 目标设备配置
CONFIG_TARGET_mediatek=y                            # 选择联发科平台
CONFIG_TARGET_mediatek_filogic=y                    # 选择联发科Filogic芯片系列
CONFIG_TARGET_mediatek_filogic_DEVICE_cmcc_rax3000m=y  # 启用中国移动RAX3000M设备支持

# 编译选项
CONFIG_USE_APK=n                                    # 不使用APK包管理
CONFIG_DEVEL=y                                      # 开发模式（保留调试信息）
CONFIG_CCACHE=y                                     # 启用编译缓存（加速重复编译）

# BUSYBOX配置
CONFIG_BUSYBOX_CUSTOM=y                             # 自定义BusyBox组件
CONFIG_BUSYBOX_CONFIG_TELNET=y                      # 启用Telnet功能

# 网络协议
CONFIG_PACKAGE_proto-bonding=y                      # 支持网络bonding（链路聚合）
CONFIG_PACKAGE_luci-proto-wireguard=y               # 支持WireGuard VPN协议的LuCI界面
CONFIG_PACKAGE_ipv6helper=y                         # IPv6辅助工具集
CONFIG_PACKAGE_luci-proto-ipv6=y                    # IPv6协议的LuCI界面支持
CONFIG_PACKAGE_6in4=y                               # 支持6in4隧道
CONFIG_PACKAGE_6to4=y                               # 支持6to4隧道
CONFIG_PACKAGE_6rd=y                                # 支持6rd隧道

# 内核模块
CONFIG_PACKAGE_kmod-fs-exfat=y                      # 支持exFAT文件系统
CONFIG_PACKAGE_kmod-fs-ext4=y                       # 支持EXT4文件系统
CONFIG_PACKAGE_kmod-fs-vfat=y                       # 支持VFAT文件系统
CONFIG_PACKAGE_kmod-mtd-rw=y                        # 支持MTD设备可写
CONFIG_PACKAGE_kmod-netlink-diag=y                  # 支持netlink诊断
CONFIG_PACKAGE_kmod-inet-diag=y                     # 支持INET诊断
CONFIG_PACKAGE_kmod-tls=y                           # 支持TLS内核加速
CONFIG_PACKAGE_kmod-tun=y                           # 支持TUN/TAP虚拟网络设备
CONFIG_KERNEL_IPV6=y                                # 内核启用IPv6支持
CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=y                # 内核支持IPv6多路由表
CONFIG_PACKAGE_kmod-ipv6=y                          # IPv6内核模块
CONFIG_PACKAGE_kmod-ip6tables=y                     # IPv6防火墙内核模块
CONFIG_PACKAGE_kmod-ip6t-NULL=y                     # IPv6空目标匹配模块
CONFIG_PACKAGE_kmod-ip6t-mangle=y                   # IPv6数据包修改模块
CONFIG_PACKAGE_kmod-ip6t-reject=y                   # IPv6拒绝策略模块

# USB支持
CONFIG_PACKAGE_kmod-usb-acm=y                       # 支持USB ACM设备（调制解调器）
CONFIG_PACKAGE_kmod-usb-ehci=y                      # 支持USB EHCI控制器
CONFIG_PACKAGE_kmod-usb-ohci=y                      # 支持USB OHCI控制器
# CONFIG_PACKAGE_kmod-usb-serial-qualcomm=n           # 支持高通USB串口设备
CONFIG_PACKAGE_kmod-usb-storage=y                   # 支持USB存储设备
CONFIG_PACKAGE_kmod-usb-storage-extras=y            # 额外的USB存储支持
CONFIG_PACKAGE_kmod-usb-storage-uas=y               # 支持USB附加存储协议
CONFIG_PACKAGE_kmod-usb2=y                          # 支持USB 2.0

# 库文件
CONFIG_PACKAGE_luci-lib-ipkg=y                      # LuCI的ipkg库支持
CONFIG_PACKAGE_libopenssl-legacy=y                  # 兼容旧版的OpenSSL库
CONFIG_PACKAGE_libip6tc=y                           # IPv6表库

# 禁用的软件包
CONFIG_PACKAGE_automount=y                          # 禁用自动挂载功能

# 应用程序
CONFIG_PACKAGE_easytier=y                           # 网络加速工具
CONFIG_PACKAGE_htop=y                               # 系统监控工具
CONFIG_PACKAGE_fuse-utils=y                         # FUSE文件系统工具
CONFIG_PACKAGE_ntfs3-mount=y                        # NTFS3文件系统挂载工具
CONFIG_PACKAGE_openssh-sftp-server=y                # SSH SFTP服务器
CONFIG_PACKAGE_tcpdump=y                            # 网络抓包工具
# CONFIG_PACKAGE_sgdisk=n                             # 磁盘分区工具
CONFIG_PACKAGE_openssl-util=y                       # OpenSSL工具
CONFIG_PACKAGE_resize2fs=y                          # 文件系统扩容工具
# CONFIG_PACKAGE_qrencode=n                           # QR码生成工具
# CONFIG_PACKAGE_smartmontools-drivedb=n              # 硬盘监控工具数据库
CONFIG_PACKAGE_usbutils=y                           # USB设备工具
# CONFIG_PACKAGE_usbmuxd=n                            # 苹果设备连接工具
CONFIG_PACKAGE_default-settings=y                   # 默认系统设置
CONFIG_PACKAGE_default-settings-chn=y               # 中文环境默认设置
CONFIG_PACKAGE_ip6tables-nft=y                      # NFT版本的ip6tables
CONFIG_PACKAGE_ip6tables-utils=y                    # IPv6防火墙工具

# 防火墙
CONFIG_PACKAGE_iptables-nft=y                       # NFT版本的iptables
CONFIG_PACKAGE_firewall4=y                          # 支持IPv6的防火墙4
CONFIG_PACKAGE_firewall4-iptables=y                 # 防火墙4的iptables兼容层
CONFIG_PACKAGE_luci-app-firewall=y                  # 防火墙的LuCI界面
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y          # IPv6全锥形NAT模块

# mwan3依赖
CONFIG_PACKAGE_iptables-mod-conntrack-extra=y       # 额外的连接跟踪模块
CONFIG_PACKAGE_iptables-mod-ipopt=y                 # IP选项匹配模块

# 默认LuCI应用
# CONFIG_PACKAGE_luci-app-adguardhome=n                # AdGuardHome广告过滤
# CONFIG_PACKAGE_luci-app-adguardhome_INCLUDE_binary=n # 不包含AdGuardHome二进制文件
CONFIG_PACKAGE_luci-app-autoreboot=y                # 自动重启应用
# CONFIG_PACKAGE_luci-app-diskman=n                   # 磁盘管理应用
CONFIG_PACKAGE_luci-app-easymesh=y
CONFIG_PACKAGE_luci-app-istorex=y                   # 软件商店
CONFIG_PACKAGE_luci-app-lucky=y                     # 网络工具
# CONFIG_PACKAGE_luci-app-quickfile=n                 # 文件快速访问
CONFIG_PACKAGE_luci-app-samba4=y                    # Samba文件共享
# CONFIG_PACKAGE_luci-app-smartdns=n                 # SmartDNS域名解析（支持IPv6）
CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_luci-app-ttyd=y                      # 网页终端
# CONFIG_PACKAGE_luci-app-vlmcsd=n                    # KMS激活服务
CONFIG_PACKAGE_luci-app-wol=y                       # 网络唤醒
CONFIG_PACKAGE_luci-app-package-manager=y           # 包管理应用
CONFIG_PACKAGE_luci-proto-ppp=y                     # PPP协议支持（含IPv6）
CONFIG_PACKAGE_luci-app-argon-config=y 

# 新增插件
CONFIG_PACKAGE_luci-app-openclash=y                 # OpenClash代理工具（支持IPv6）
CONFIG_PACKAGE_luci-app-timecontrol=y               # 时间控制应用
CONFIG_PACKAGE_luci-app-ipv6-helper=y               # IPv6辅助配置工具

