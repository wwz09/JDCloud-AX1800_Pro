# 目标设备配置
CONFIG_TARGET_qualcommax=y                          # 高通max平台
CONFIG_TARGET_qualcommax_ipq60xx=y                  # IPQ60xx芯片系列
CONFIG_TARGET_MULTI_PROFILE=y                       # 多配置文件支持
CONFIG_TARGET_PER_DEVICE_ROOTFS=y                   # 独立根文件系统
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_qihoo_360v6=y  # 360V6路由器

# 编译选项
CONFIG_CCACHE=y                                     # 启用编译缓存

# 基础工具配置
CONFIG_BUSYBOX_CUSTOM=y                             # 自定义BusyBox
CONFIG_BUSYBOX_CONFIG_TELNET=y                      # 启用Telnet
CONFIG_PACKAGE_zram-swap=m                          # zram交换分区(模块化)

# NSS硬件加速
CONFIG_IPQ_MEM_PROFILE_256=y                        # IPQ内存配置(256MB)
CONFIG_ATH11K_MEM_PROFILE_512M=y                    # 无线网卡内存(512MB)
CONFIG_NSS_MEM_PROFILE_HIGH=y                       # NSS高性能内存配置
CONFIG_NSS_FIRMWARE_VERSION_11_4=y                  # NSS固件v11.4
CONFIG_PACKAGE_sqm-scripts-nss=y                    # NSS加速SQM
CONFIG_PACKAGE_kmod-qca-mcs=y                       # QCA MCS模块
CONFIG_KERNEL_SKB_RECYCLER=y                        # 内核SKB缓存回收
CONFIG_KERNEL_SKB_RECYCLER_MULTI_CPU=y              # 多CPU SKB回收
CONFIG_PACKAGE_kmod-ath11k-pci=m                    # ATH11K无线PCI驱动(模块化)
CONFIG_NSS_DRV_WIFI_MESH_ENABLE=y                   # NSS驱动Mesh支持
CONFIG_PACKAGE_MAC80211_MESH=y                      # MAC80211 Mesh功能
CONFIG_ATH11K_NSS_MESH_SUPPORT=y                    # ATH11K NSS Mesh加速

# 网络协议与VPN（含IPv6）
CONFIG_PACKAGE_proto-bonding=y                      # 网络bonding(链路聚合)
CONFIG_PACKAGE_luci-proto-wireguard=y               # WireGuard LuCI界面(支持IPv6)
CONFIG_PACKAGE_luci-proto-relay=y                   # 中继协议LuCI界面(支持IPv6)
CONFIG_PACKAGE_ipv6helper=y                         # IPv6辅助工具集
CONFIG_PACKAGE_luci-proto-ipv6=y                    # IPv6协议的LuCI界面支持
CONFIG_PACKAGE_6in4=y                               # 6in4隧道支持
CONFIG_PACKAGE_6to4=y                               # 6to4隧道支持
CONFIG_PACKAGE_6rd=y                                # 6rd隧道支持

# 内核模块（含IPv6）
# 核心IPv6支持
CONFIG_KERNEL_IPV6=y                                # 内核启用IPv6协议栈
CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=y                # 内核支持IPv6多路由表
CONFIG_PACKAGE_kmod-ipv6=y                          # IPv6核心内核模块
CONFIG_PACKAGE_kmod-ip6tables=y                     # IPv6防火墙内核模块
CONFIG_PACKAGE_kmod-ip6t-mangle=y                   # IPv6数据包修改模块
CONFIG_PACKAGE_kmod-ip6t-reject=y                   # IPv6拒绝策略模块

# 文件系统支持
CONFIG_PACKAGE_kmod-fs-exfat=y                      # exFAT支持
CONFIG_PACKAGE_kmod-fs-vfat=y                       # VFAT支持

# 网络诊断
CONFIG_PACKAGE_kmod-netlink-diag=y                  # netlink诊断
CONFIG_PACKAGE_kmod-inet-diag=y                     # INET诊断(含IPv6)

# 网络加速与虚拟设备
CONFIG_PACKAGE_kmod-tls=y                           # TLS内核加速(支持IPv6)
CONFIG_PACKAGE_kmod-tun=y                           # TUN/TAP支持(支持IPv6)

# USB设备支持
# 控制器
CONFIG_PACKAGE_kmod-usb-ehci=y                      # USB EHCI控制器
CONFIG_PACKAGE_kmod-usb-ohci=y                      # USB OHCI控制器
CONFIG_PACKAGE_kmod-usb2=y                          # USB 2.0支持

# 存储与串口
CONFIG_PACKAGE_kmod-usb-storage=y                   # USB存储
CONFIG_PACKAGE_kmod-usb-storage-extras=y            # 额外USB存储支持
CONFIG_PACKAGE_kmod-usb-storage-uas=y               # USB附加存储协议
CONFIG_PACKAGE_kmod-usb-serial-qualcomm=y           # 高通USB串口
CONFIG_PACKAGE_kmod-usb-acm=y                       # USB ACM设备(调制解调器)

# NSS内核驱动（支持IPv6加速）
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y               # NSS IGS支持(含IPv6)
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=y            # NSS L2TPv2支持(含IPv6)
CONFIG_PACKAGE_kmod-qca-nss-drv-lag-mgr=y           # NSS链路聚合管理(含IPv6)
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y             # NSS PPPoE支持(含IPv6)
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=y              # NSS PPTP支持(含IPv6)

# 库文件（含IPv6）
CONFIG_PACKAGE_luci-lib-ipkg=y                      # LuCI ipkg库
CONFIG_PACKAGE_libopenssl-legacy=y                  # OpenSSL旧版兼容库
CONFIG_PACKAGE_libip6tc=y                           # IPv6表操作库

# 应用工具（含IPv6支持）
# 系统工具
CONFIG_PACKAGE_htop=y                               # 系统监控
CONFIG_PACKAGE_fuse-utils=y                         # FUSE工具
CONFIG_PACKAGE_ntfs3-mount=y                        # NTFS3挂载
CONFIG_PACKAGE_openssh-sftp-server=y                # SFTP服务器(支持IPv6)
CONFIG_PACKAGE_tcpdump=y                            # 网络抓包(支持IPv6)
CONFIG_PACKAGE_sgdisk=y                             # 磁盘分区
CONFIG_PACKAGE_openssl-util=y                       # OpenSSL工具(支持IPv6)
CONFIG_PACKAGE_resize2fs=y                          # 文件系统扩容
CONFIG_PACKAGE_qrencode=y                           # QR码生成
CONFIG_PACKAGE_smartmontools-drivedb=y              # 硬盘监控数据库
CONFIG_PACKAGE_usbutils=y                           # USB设备工具
# CONFIG_PACKAGE_usbmuxd=n                            # 苹果设备连接
CONFIG_PACKAGE_mii-tool=y                           # 网络接口配置
CONFIG_PACKAGE_default-settings=y                   # 默认系统设置
CONFIG_PACKAGE_default-settings-chn=y               # 中文环境设置

# 网络加速
CONFIG_PACKAGE_easytier=y                           # 网络加速工具(支持IPv6)

# 防火墙(完整IPv6支持)
CONFIG_PACKAGE_iptables-nft=y                       # NFT版iptables
CONFIG_PACKAGE_ip6tables-nft=y                      # NFT版ip6tables
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y          # IPv6全锥形NAT
CONFIG_PACKAGE_firewall4=y                          # 新一代防火墙(原生支持IPv6)
CONFIG_PACKAGE_firewall4-iptables=y                 # 防火墙兼容层
CONFIG_PACKAGE_luci-app-firewall=y                  # 防火墙LuCI界面(支持IPv6配置)

# LuCI应用（含IPv6支持）
# 系统管理
CONFIG_PACKAGE_luci-app-autoreboot=y                # 自动重启
CONFIG_PACKAGE_luci-app-diskman=y                   # 磁盘管理
CONFIG_PACKAGE_luci-app-ttyd=y                      # 网页终端
CONFIG_PACKAGE_luci-app-package-manager=y           # 包管理

# 网络工具
CONFIG_PACKAGE_luci-app-sqm=y                       # 流量控制(支持IPv6)
CONFIG_PACKAGE_luci-app-wol=y                       # 网络唤醒(支持IPv6)
CONFIG_PACKAGE_luci-app-lucky=y                     # 网络工具(支持IPv6)
CONFIG_PACKAGE_luci-app-oaf=y                       # 应用过滤(支持IPv6)
CONFIG_PACKAGE_luci-app-ipv6-helper=y               # IPv6配置辅助工具

# 文件服务
CONFIG_PACKAGE_luci-app-samba4=y                    # Samba共享(支持IPv6)
CONFIG_PACKAGE_luci-app-quickfile=y                 # 文件快速访问

# 其他工具
CONFIG_PACKAGE_luci-app-istorex=y                   # 软件商店
# CONFIG_PACKAGE_luci-app-vlmcsd=n                    # KMS激活服务

# 新增插件（支持IPv6）
# CONFIG_PACKAGE_luci-app-openclash=n                 # OpenClash代理(支持IPv6)
CONFIG_PACKAGE_luci-app-timecontrol=y               # 时间控制(支持IPv6)

