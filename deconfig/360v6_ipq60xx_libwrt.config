# 目标设备配置
CONFIG_TARGET_qualcommax=y                          # 启用高通max平台支持
CONFIG_TARGET_qualcommax_ipq60xx=y                  # 启用IPQ60xx芯片系列支持
CONFIG_TARGET_MULTI_PROFILE=y                       # 启用多配置文件支持
CONFIG_TARGET_PER_DEVICE_ROOTFS=y                   # 启用每个设备独立的根文件系统
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_qihoo_360v6=y  # 针对360V6路由器的设备配置

# 编译选项
CONFIG_USE_APK=n                                    # 不使用APK包管理
CONFIG_DEVEL=y                                      # 启用开发模式
CONFIG_CCACHE=y                                     # 启用编译器缓存，加速编译

# BUSYBOX配置
CONFIG_BUSYBOX_CUSTOM=y                             # 启用自定义BusyBox配置
CONFIG_BUSYBOX_CONFIG_TELNET=y                      # 在BusyBox中启用Telnet支持

# 交换分区支持
CONFIG_PACKAGE_zram-swap=m                          # 模块化编译zram-swap支持

# NSS（网络加速引擎）配置
CONFIG_IPQ_MEM_PROFILE_256=y                        # 配置IPQ内存为256MB模式
CONFIG_ATH11K_MEM_PROFILE_512M=y                    # 配置ATH11K无线驱动内存为512MB模式
CONFIG_NSS_MEM_PROFILE_HIGH=y                       # 配置NSS使用高内存模式
CONFIG_NSS_FIRMWARE_VERSION_11_4=y                  # 使用11.4版本的NSS固件
CONFIG_PACKAGE_sqm-scripts-nss=y                    # 启用NSS支持的SQM脚本
CONFIG_PACKAGE_kmod-qca-mcs=y                       # 启用QCA MCS内核模块
CONFIG_KERNEL_SKB_RECYCLER=y                        # 启用内核SKB缓存回收器
CONFIG_KERNEL_SKB_RECYCLER_MULTI_CPU=y              # 启用多CPU的SKB缓存回收
CONFIG_PACKAGE_kmod-ath11k-pci=m                    # 模块化编译ATH11K PCI驱动
CONFIG_NSS_DRV_WIFI_MESH_ENABLE=y                   # 启用NSS驱动的WiFi Mesh支持
CONFIG_PACKAGE_MAC80211_MESH=y                      # 启用MAC80211 Mesh支持
CONFIG_ATH11K_NSS_MESH_SUPPORT=y                    # 启用ATH11K的NSS Mesh支持

# 网络协议支持
CONFIG_PACKAGE_proto-bonding=y                      # 启用网络绑定协议支持
# CONFIG_PACKAGE_luci-proto-quectel=y               # 禁用Quectel调制解调器协议支持（注释掉）
CONFIG_PACKAGE_luci-proto-wireguard=y               # 启用WireGuard VPN协议的LuCI支持
CONFIG_PACKAGE_luci-proto-relay=y                   # 启用中继协议的LuCI支持

# 内核模块
CONFIG_PACKAGE_kmod-fs-exfat=y                      # 启用exFAT文件系统支持
CONFIG_PACKAGE_kmod-fs-vfat=y                       # 启用VFAT文件系统支持
CONFIG_PACKAGE_kmod-netlink-diag=y                  # 启用netlink诊断模块
CONFIG_PACKAGE_kmod-inet-diag=y                     # 启用INET诊断模块
CONFIG_PACKAGE_kmod-tls=y                           # 启用TLS内核支持
CONFIG_PACKAGE_kmod-tun=y                           # 启用TUN/TAP虚拟网络设备支持

# USB支持
CONFIG_PACKAGE_kmod-usb-acm=y                       # 启用USB ACM调制解调器支持
CONFIG_PACKAGE_kmod-usb-ehci=y                      # 启用USB EHCI主机控制器支持
# CONFIG_PACKAGE_kmod-usb-net-huawei-cdc-ncm=n        # 启用华为CDC NCM USB网卡支持
# CONFIG_PACKAGE_kmod-usb-net-ipheth=n                # 启用iPhone USB以太网支持
# CONFIG_PACKAGE_kmod-usb-net-rndis=n                 # 启用USB RNDIS网卡支持
# CONFIG_PACKAGE_kmod-usb-net-asix-ax88179=n          # 启用ASIX AX88179 USB网卡支持
# CONFIG_PACKAGE_kmod-usb-net-rtl8152=n               # 启用Realtek 8152 USB网卡支持
# CONFIG_PACKAGE_kmod-usb-net-sierrawireless=n        # 启用Sierra Wireless USB网卡支持
CONFIG_PACKAGE_kmod-usb-ohci=y                      # 启用USB OHCI主机控制器支持
# CONFIG_PACKAGE_kmod-usb-serial-qualcomm=n           # 启用高通USB串口支持
CONFIG_PACKAGE_kmod-usb-storage=y                   # 启用USB存储设备支持
CONFIG_PACKAGE_kmod-usb-storage-extras=y            # 启用额外的USB存储设备支持
CONFIG_PACKAGE_kmod-usb-storage-uas=y               # 启用USB附加存储设备支持
CONFIG_PACKAGE_kmod-usb2=y                          # 启用USB 2.0支持

# Docker内核依赖
CONFIG_PACKAGE_kmod-br-netfilter=y                  # 启用桥接网络的netfilter支持
CONFIG_PACKAGE_kmod-ip6tables=y                     # 启用IPv6防火墙支持
CONFIG_PACKAGE_kmod-ipt-conntrack=y                 # 启用iptables连接跟踪支持
CONFIG_PACKAGE_kmod-ipt-extra=y                     # 启用额外的iptables模块
CONFIG_PACKAGE_kmod-ipt-nat=y                       # 启用iptables NAT支持
CONFIG_PACKAGE_kmod-ipt-nat6=y                      # 启用IPv6 NAT支持
CONFIG_PACKAGE_kmod-ipt-physdev=y                   # 启用iptables物理设备匹配支持
CONFIG_PACKAGE_kmod-nf-ipt6=y                       # 启用IPv6的netfilter iptables支持
CONFIG_PACKAGE_kmod-nf-ipvs=y                       # 启用IP虚拟服务器支持
CONFIG_PACKAGE_kmod-nf-nat6=y                       # 启用IPv6 NAT支持
CONFIG_PACKAGE_kmod-dummy=y                         # 启用虚拟网络接口支持
CONFIG_PACKAGE_kmod-veth=y                          # 启用虚拟以太网设备支持

# mwan3多WAN依赖
CONFIG_PACKAGE_kmod-macvlan=y                       # 启用MAC VLAN支持

# NSS内核驱动
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y               # 启用NSS驱动的IGS支持
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=y            # 启用NSS驱动的L2TPv2支持
CONFIG_PACKAGE_kmod-qca-nss-drv-lag-mgr=y           # 启用NSS驱动的链路聚合管理支持
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y             # 启用NSS驱动的PPPoE支持
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=y              # 启用NSS驱动的PPTP支持

# 库文件
CONFIG_PACKAGE_luci-lib-ipkg=y                      # 启用LuCI的ipkg库支持
CONFIG_PACKAGE_libopenssl-legacy=y                  # 启用旧版OpenSSL库支持

# 应用程序包
CONFIG_PACKAGE_easytier=y                           # 启用easytier网络加速工具
CONFIG_PACKAGE_htop=y                               # 启用htop系统监控工具
CONFIG_PACKAGE_fuse-utils=y                         # 启用FUSE文件系统工具
CONFIG_PACKAGE_ntfs3-mount=y                        # 启用NTFS3文件系统挂载支持
CONFIG_PACKAGE_openssh-sftp-server=y                # 启用OpenSSH SFTP服务器
CONFIG_PACKAGE_tcpdump=y                            # 启用tcpdump网络抓包工具
# CONFIG_PACKAGE_sgdisk=n                             # 启用sgdisk磁盘分区工具
CONFIG_PACKAGE_openssl-util=y                       # 启用OpenSSL工具
CONFIG_PACKAGE_resize2fs=y                          # 启用resize2fs文件系统调整工具
# CONFIG_PACKAGE_qrencode=n                           # 启用QR码生成工具
# CONFIG_PACKAGE_smartmontools-drivedb=n              # 启用智能硬盘监控工具数据库
CONFIG_PACKAGE_usbutils=y                           # 启用USB设备工具
# CONFIG_PACKAGE_usbmuxd=n                            # 启用usbmuxd（iOS设备连接工具）
# CONFIG_PACKAGE_mii-tool=n                           # 启用MII网络接口工具
CONFIG_PACKAGE_default-settings=y                   # 启用默认设置
CONFIG_PACKAGE_default-settings-chn=y               # 启用中国地区默认设置

# Coremark性能测试
