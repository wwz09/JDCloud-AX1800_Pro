# 目标设备配置
CONFIG_TARGET_qualcommax=y                          # 选择高通max平台
CONFIG_TARGET_qualcommax_ipq60xx=y                  # 选择IPQ60xx芯片系列
CONFIG_TARGET_MULTI_PROFILE=y                       # 支持多配置文件
CONFIG_TARGET_PER_DEVICE_ROOTFS=y                   # 为每个设备生成独立的根文件系统
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_qihoo_360v6=y  # 具体设备：360V6路由器

# 编译选项
CONFIG_USE_APK=n                                    # 不使用APK包管理
CONFIG_DEVEL=y                                      # 开发模式（保留更多调试信息）
CONFIG_CCACHE=y                                     # 启用编译缓存（加速重复编译）

# BusyBox配置
CONFIG_BUSYBOX_CUSTOM=y                             # 自定义BusyBox组件
CONFIG_BUSYBOX_CONFIG_TELNET=y                      # 启用Telnet功能

# 交换分区支持
CONFIG_PACKAGE_zram-swap=m                          # 模块化编译zram交换分区支持

# NSS硬件加速配置
CONFIG_IPQ_MEM_PROFILE_256=y                        # IPQ芯片内存配置：256MB
CONFIG_ATH11K_MEM_PROFILE_512M=y                    # 无线网卡内存配置：512MB
CONFIG_NSS_MEM_PROFILE_HIGH=y                       # NSS高性能内存配置
CONFIG_NSS_FIRMWARE_VERSION_11_4=y                  # NSS固件版本11.4
CONFIG_PACKAGE_sqm-scripts-nss=y                    # 启用NSS加速的SQM流量管理
CONFIG_PACKAGE_kmod-qca-mcs=y                       # 启用QCA MCS模块
CONFIG_KERNEL_SKB_RECYCLER=y                        # 启用内核SKB缓存回收
CONFIG_KERNEL_SKB_RECYCLER_MULTI_CPU=y              # 支持多CPU的SKB缓存回收
CONFIG_PACKAGE_kmod-ath11k-pci=m                    # 模块化编译ATH11K无线PCI驱动
CONFIG_NSS_DRV_WIFI_MESH_ENABLE=y                   # 启用NSS驱动的WiFi Mesh支持
CONFIG_PACKAGE_MAC80211_MESH=y                      # 启用MAC80211 Mesh功能
CONFIG_ATH11K_NSS_MESH_SUPPORT=y                    # ATH11K支持NSS加速的Mesh

# 网络协议支持（保留IPv6相关）
CONFIG_PACKAGE_proto-bonding=y                      # 支持网络 bonding（链路聚合）
CONFIG_PACKAGE_luci-proto-wireguard=y               # 支持WireGuard VPN协议的LuCI界面
CONFIG_PACKAGE_luci-proto-relay=y                   # 支持中继协议的LuCI界面

# 内核模块
CONFIG_PACKAGE_kmod-fs-exfat=y                      # 支持exFAT文件系统
CONFIG_PACKAGE_kmod-fs-vfat=y                       # 支持VFAT文件系统
CONFIG_PACKAGE_kmod-netlink-diag=y                  # 支持netlink诊断
CONFIG_PACKAGE_kmod-inet-diag=y                     # 支持INET诊断
CONFIG_PACKAGE_kmod-tls=y                           # 支持TLS内核加速
CONFIG_PACKAGE_kmod-tun=y                           # 支持TUN/TAP虚拟网络设备

# USB设备支持
CONFIG_PACKAGE_kmod-usb-acm=y                       # 支持USB ACM设备（调制解调器）
CONFIG_PACKAGE_kmod-usb-ehci=y                      # 支持USB EHCI控制器
CONFIG_PACKAGE_kmod-usb-net-huawei-cdc-ncm=y        # 支持华为USB网卡
CONFIG_PACKAGE_kmod-usb-net-ipheth=y                # 支持苹果USB以太网
CONFIG_PACKAGE_kmod-usb-net-rndis=y                 # 支持RNDIS协议USB网卡
CONFIG_PACKAGE_kmod-usb-net-asix-ax88179=y          # 支持ASIX AX88179 USB网卡
CONFIG_PACKAGE_kmod-usb-net-rtl8152=y               # 支持RTL8152 USB网卡
CONFIG_PACKAGE_kmod-usb-net-sierrawireless=y        # 支持 Sierra 无线USB网卡
CONFIG_PACKAGE_kmod-usb-ohci=y                      # 支持USB OHCI控制器
CONFIG_PACKAGE_kmod-usb-serial-qualcomm=y           # 支持高通USB串口设备
CONFIG_PACKAGE_kmod-usb-storage=y                   # 支持USB存储设备
CONFIG_PACKAGE_kmod-usb-storage-extras=y            # 额外的USB存储支持
CONFIG_PACKAGE_kmod-usb-storage-uas=y               # 支持USB附加存储协议
CONFIG_PACKAGE_kmod-usb2=y                          # 支持USB 2.0

# mwan3多线负载均衡依赖（保留）
CONFIG_PACKAGE_kmod-macvlan=y                       # 支持MAC VLAN

# NSS内核驱动
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y               # NSS驱动支持IGS
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=y            # NSS驱动支持L2TPv2
CONFIG_PACKAGE_kmod-qca-nss-drv-lag-mgr=y           # NSS驱动支持链路聚合管理
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y             # NSS驱动支持PPPoE
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=y              # NSS驱动支持PPTP

# 库文件
CONFIG_PACKAGE_luci-lib-ipkg=y                      # LuCI的ipkg库支持
CONFIG_PACKAGE_libopenssl-legacy=y                  # 兼容旧版的OpenSSL库

# 应用程序
CONFIG_PACKAGE_easytier=y                           # 网络加速工具
CONFIG_PACKAGE_htop=y                               # 系统监控工具
CONFIG_PACKAGE_fuse-utils=y                         # FUSE文件系统工具
CONFIG_PACKAGE_ntfs3-mount=y                        # NTFS3文件系统挂载工具
CONFIG_PACKAGE_openssh-sftp-server=y                # SSH SFTP服务器
CONFIG_PACKAGE_tcpdump=y                            # 网络抓包工具
CONFIG_PACKAGE_sgdisk=y                             # 磁盘分区工具
CONFIG_PACKAGE_openssl-util=y                       # OpenSSL工具
CONFIG_PACKAGE_resize2fs=y                          # 文件系统 resize 工具
CONFIG_PACKAGE_qrencode=y                           # QR码生成工具
CONFIG_PACKAGE_smartmontools-drivedb=y              # 硬盘监控工具数据库
CONFIG_PACKAGE_usbutils=y                           # USB设备工具
CONFIG_PACKAGE_usbmuxd=y                            # 苹果设备连接工具
CONFIG_PACKAGE_mii-tool=y                           # 网络接口配置工具
CONFIG_PACKAGE_default-settings=y                   # 默认系统设置
CONFIG_PACKAGE_default-settings-chn=y               # 中文环境默认设置

# 性能测试工具
CONFIG_PACKAGE_coremark=y                           # 处理器性能测试工具
CONFIG_COREMARK_OPTIMIZE_O3=y                       # 启用O3优化
CONFIG_COREMARK_ENABLE_MULTITHREADING=y             # 支持多线程测试
CONFIG_COREMARK_NUMBER_OF_THREADS=6                 # 测试线程数：6

# 防火墙工具（保留IPv6相关）
CONFIG_PACKAGE_iptables-nft=y                       # NFT版本的iptables
CONFIG_PACKAGE_ip6tables-nft=y                      # NFT版本的ip6tables（保留IPv6）
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y          # IPv6全锥形NAT模块（保留）
CONFIG_PACKAGE_libip6tc=y                           # IPv6表库（保留）

# mwan3依赖工具
CONFIG_PACKAGE_iptables-mod-conntrack-extra=y       # 额外的连接跟踪模块

# 默认LuCI应用
CONFIG_PACKAGE_luci-app-autoreboot=y                # 自动重启应用
CONFIG_PACKAGE_luci-app-diskman=y                   # 磁盘管理应用
CONFIG_PACKAGE_luci-app-istorex=y                   # 软件商店
CONFIG_PACKAGE_luci-app-lucky=y                     # 网络工具
CONFIG_PACKAGE_luci-app-oaf=y                       # 应用过滤
CONFIG_PACKAGE_luci-app-quickfile=y                 # 文件快速访问
CONFIG_PACKAGE_luci-app-samba4=y                    # Samba文件共享
CONFIG_PACKAGE_luci-app-sqm=y                       # 流量控制
CONFIG_PACKAGE_luci-app-ttyd=y                      # 网页终端
CONFIG_PACKAGE_luci-app-vlmcsd=y                    # KMS激活服务
CONFIG_PACKAGE_luci-app-wol=y                       # 网络唤醒
CONFIG_PACKAGE_luci-app-package-manager=y           # 包管理应用

# 新增插件
CONFIG_PACKAGE_luci-app-openclash=y                 # OpenClash代理工具
CONFIG_PACKAGE_luci-app-timecontrol=y               # 时间控制应用

