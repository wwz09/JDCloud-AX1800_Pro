# iptables与nftables
CONFIG_PACKAGE_iptables-nft=y                       # 启用基于nftables的iptables支持
CONFIG_PACKAGE_ip6tables-nft=y                      # 启用基于nftables的ip6tables支持

# Docker依赖
CONFIG_PACKAGE_iptables-mod-extra=y                 # 启用额外的iptables模块
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y          # 启用IPv6的fullcone NAT支持
CONFIG_PACKAGE_iptables-mod-fullconenat=y           # 启用IPv4的fullcone NAT支持
CONFIG_PACKAGE_libip4tc=y                           # 启用IPv4表库
CONFIG_PACKAGE_libip6tc=y                           # 启用IPv6表库

# mwan3依赖
CONFIG_PACKAGE_iptables-mod-conntrack-extra=y       # 启用额外的连接跟踪模块

