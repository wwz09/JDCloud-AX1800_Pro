################################################################################
# 京东云 IPQ60XX 系列路由器 OpenWrt 编译配置文件
# 特点：完整的 IPv6 支持、NSS 硬件加速、多设备支持
################################################################################

# ===============================================================================
# 1. 目标设备配置
# ===============================================================================
CONFIG_TARGET_qualcommax=y                          # 选择高通max平台作为目标架构
CONFIG_TARGET_qualcommax_ipq60xx=y                  # 选择IPQ60xx芯片系列作为具体目标
CONFIG_TARGET_MULTI_PROFILE=y                       # 启用多设备配置文件支持
CONFIG_TARGET_PER_DEVICE_ROOTFS=y                   # 为每个设备生成独立的根文件系统
CONFIG_TARGET_ROOTFS_INITRAMFS=n                    # 不使用内存文件系统(initramfs)，使用常规rootfs

# ===============================================================================
# 2. 具体设备配置 - 京东云系列路由器
# ===============================================================================
# 京东云re-ss-01设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01="ipq-wifi-jdcloud_re-ss-01 -kmod-ath11k-pci zram-swap"

# 京东云re-cs-02设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="ipq-wifi-jdcloud_re-cs-02 kmod-ath11k-pci ath11k-firmware-qcn9074 luci-app-athena-led luci-i18n-athena-led-zh-cn luci-app-cupsd"

# 京东云re-cs-07设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-07=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-07="-ath11k-firmware-ipq6018 -kmod-ath11k -kmod-ath11k-ahb -kmod-ath11k-pci luci-app-dockerman luci-i18n-dockerman-zh-cn"

# 红米AX5京东云版设备
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_redmi_ax5-jdcloud=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_redmi_ax5-jdcloud="ipq-wifi-redmi_ax5-jdcloud -kmod-ath11k-pci zram-swap"

# ===============================================================================
# 3. 编译选项
# ===============================================================================
CONFIG_USE_APK=n                                    # 不使用APK包管理
CONFIG_DEVEL=y                                      # 开发模式（保留调试信息，便于问题排查）
CONFIG_CCACHE=y                                     # 启用编译缓存（显著加速重复编译过程）

# ===============================================================================
# 4. BusyBox配置
# ===============================================================================
CONFIG_BUSYBOX_CUSTOM=y                             # 自定义BusyBox组件
CONFIG_BUSYBOX_CONFIG_TELNET=y                      # 启用Telnet功能（远程管理备用方案）

# ===============================================================================
# 5. 系统增强功能
# ===============================================================================
CONFIG_PACKAGE_zram-swap=m                          # 模块化编译zram交换分区支持（提高内存利用率）

# ===============================================================================
# 6. NSS硬件加速配置（完整支持IPv6加速）
# ===============================================================================
CONFIG_IPQ_MEM_PROFILE_256=y                        # IPQ芯片内存配置：256MB
CONFIG_ATH11K_MEM_PROFILE_512M=y                    # 无线网卡内存配置：512MB
CONFIG_NSS_MEM_PROFILE_HIGH=y                       # NSS高性能内存配置
CONFIG_NSS_FIRMWARE_VERSION_11_4=y                  # NSS固件版本11.4（支持IPv6硬件加速）
CONFIG_PACKAGE_sqm-scripts-nss=y                    # 启用NSS加速的SQM流量管理（支持IPv6）
CONFIG_PACKAGE_kmod-qca-mcs=y                       # 启用QCA MCS模块（多核心调度）
CONFIG_KERNEL_SKB_RECYCLER=y                        # 启用内核SKB缓存回收（提高网络性能）
CONFIG_KERNEL_SKB_RECYCLER_MULTI_CPU=y              # 支持多CPU的SKB缓存回收（多核心优化）
CONFIG_PACKAGE_kmod-ath11k-pci=m                    # 模块化编译ATH11K无线PCI驱动
CONFIG_NSS_DRV_WIFI_MESH_ENABLE=y                   # 启用NSS驱动的WiFi Mesh支持（含IPv6）
CONFIG_PACKAGE_MAC80211_MESH=y                      # 启用MAC80211 Mesh功能（含IPv6）
CONFIG_ATH11K_NSS_MESH_SUPPORT=y                    # ATH11K支持NSS加速的Mesh（含IPv6）

# ===============================================================================
# 7. 网络协议支持（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_proto-bonding=y                      # 支持网络bonding（链路聚合，同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-proto-wireguard=y               # 支持WireGuard VPN协议的LuCI界面（支持IPv6）
CONFIG_PACKAGE_luci-proto-relay=y                   # 支持中继协议的LuCI界面（支持IPv6）
CONFIG_PACKAGE_ipv6helper=y                         # IPv6辅助工具集（简化IPv6配置）
CONFIG_PACKAGE_luci-proto-ipv6=y                    # IPv6协议的LuCI界面支持
CONFIG_PACKAGE_6in4=y                               # 支持6in4隧道（IPv6过渡技术）
CONFIG_PACKAGE_6to4=y                               # 支持6to4隧道（IPv6过渡技术）
CONFIG_PACKAGE_6rd=y                                # 支持6rd隧道（IPv6过渡技术）

# ===============================================================================
# 8. 内核模块（完整IPv6支持）
# ===============================================================================
# 核心IPv6配置
CONFIG_KERNEL_IPV6=y                                # 内核启用IPv6协议栈
CONFIG_KERNEL_IPV6_MULTIPLE_TABLES=y                # 内核支持IPv6多路由表（策略路由）
CONFIG_PACKAGE_kmod-ipv6=y                          # IPv6核心内核模块
CONFIG_PACKAGE_kmod-ip6t-mangle=y                   # IPv6数据包修改模块
CONFIG_PACKAGE_kmod-ip6t-reject=y                   # IPv6拒绝策略模块

# 文件系统支持
CONFIG_PACKAGE_kmod-fs-exfat=y                      # 支持exFAT文件系统
CONFIG_PACKAGE_kmod-fs-vfat=y                       # 支持VFAT文件系统

# 网络诊断（含IPv6）
CONFIG_PACKAGE_kmod-netlink-diag=y                  # 支持netlink诊断
CONFIG_PACKAGE_kmod-inet-diag=y                     # 支持INET诊断（同时支持IPv4/IPv6）

# 网络加速与虚拟设备（含IPv6）
CONFIG_PACKAGE_kmod-tls=y                           # 支持TLS内核加速（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-tun=y                           # 支持TUN/TAP虚拟网络设备（同时支持IPv4/IPv6）

# USB设备支持
CONFIG_PACKAGE_kmod-usb-acm=y                       # 支持USB ACM设备（调制解调器）
CONFIG_PACKAGE_kmod-usb-ehci=y                      # 支持USB EHCI控制器
CONFIG_PACKAGE_kmod-usb-ohci=y                      # 支持USB OHCI控制器
CONFIG_PACKAGE_kmod-usb-serial-qualcomm=y           # 支持高通USB串口设备
CONFIG_PACKAGE_kmod-usb-storage=y                   # 支持USB存储设备
CONFIG_PACKAGE_kmod-usb-storage-extras=y            # 额外的USB存储支持
CONFIG_PACKAGE_kmod-usb-storage-uas=y               # 支持USB附加存储协议
CONFIG_PACKAGE_kmod-usb2=y                          # 支持USB 2.0

# Docker依赖内核模块（完整IPv6支持）
CONFIG_PACKAGE_kmod-br-netfilter=y                  # 支持桥接网络过滤（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-ip6tables=y                     # 支持IPv6防火墙
CONFIG_PACKAGE_kmod-ipt-conntrack=y                 # 支持iptables连接跟踪（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-ipt-extra=y                     # 支持额外的iptables模块（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-ipt-nat=y                       # 支持iptables NAT
CONFIG_PACKAGE_kmod-ipt-nat6=y                      # 支持IPv6 NAT
CONFIG_PACKAGE_kmod-ipt-physdev=y                   # 支持物理设备匹配（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-nf-ipt6=y                       # 支持IPv6 netfilter
CONFIG_PACKAGE_kmod-nf-ipvs=y                       # 支持IP虚拟服务器（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-nf-nat6=y                       # 支持IPv6 NAT
CONFIG_PACKAGE_kmod-dummy=y                         # 支持虚拟网卡（同时支持IPv4/IPv6）
CONFIG_PACKAGE_kmod-veth=y                          # 支持虚拟以太网对（同时支持IPv4/IPv6）

# mwan3多线负载均衡依赖（含IPv6）
CONFIG_PACKAGE_kmod-macvlan=y                       # 支持MAC VLAN（同时支持IPv4/IPv6）

# NSS内核驱动（完整支持IPv6）
CONFIG_PACKAGE_kmod-qca-nss-drv-igs=y               # NSS驱动支持IGS（含IPv6硬件加速）
CONFIG_PACKAGE_kmod-qca-nss-drv-l2tpv2=y            # NSS驱动支持L2TPv2（含IPv6硬件加速）
CONFIG_PACKAGE_kmod-qca-nss-drv-lag-mgr=y           # NSS驱动支持链路聚合管理（含IPv6）
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y             # NSS驱动支持PPPoE（含IPv6硬件加速）
CONFIG_PACKAGE_kmod-qca-nss-drv-pptp=y              # NSS驱动支持PPTP（含IPv6硬件加速）

# ===============================================================================
# 9. 库文件（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_luci-lib-ipkg=y                      # LuCI的ipkg库支持
CONFIG_PACKAGE_libopenssl-legacy=y                  # 兼容旧版的OpenSSL库（同时支持IPv4/IPv6）
CONFIG_PACKAGE_libip6tc=y                           # IPv6表库（用于IPv6防火墙规则管理）
CONFIG_PACKAGE_libip4tc=y                           # IPv4表库（用于IPv4防火墙规则管理）

# ===============================================================================
# 10. 应用程序（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_easytier=y                           # 网络加速工具（同时支持IPv4/IPv6）
CONFIG_PACKAGE_htop=y                               # 系统监控工具
CONFIG_PACKAGE_fuse-utils=y                         # FUSE文件系统工具
CONFIG_PACKAGE_ntfs3-mount=y                        # NTFS3文件系统挂载工具
CONFIG_PACKAGE_openssh-sftp-server=y                # SSH SFTP服务器（同时支持IPv4/IPv6）
CONFIG_PACKAGE_tcpdump=y                            # 网络抓包工具（同时支持IPv4/IPv6）
CONFIG_PACKAGE_sgdisk=y                             # 磁盘分区工具
CONFIG_PACKAGE_openssl-util=y                       # OpenSSL工具（同时支持IPv4/IPv6）
CONFIG_PACKAGE_resize2fs=y                          # 文件系统扩容工具
CONFIG_PACKAGE_qrencode=y                           # QR码生成工具
CONFIG_PACKAGE_smartmontools-drivedb=y              # 硬盘监控工具数据库
CONFIG_PACKAGE_usbutils=y                           # USB设备工具
CONFIG_PACKAGE_usbmuxd=y                            # 苹果设备连接工具
CONFIG_PACKAGE_mii-tool=y                           # 网络接口配置工具（同时支持IPv4/IPv6）
CONFIG_PACKAGE_default-settings=y                   # 默认系统设置
CONFIG_PACKAGE_default-settings-chn=y               # 中文环境默认设置

# ===============================================================================
# 11. 防火墙工具（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_iptables-nft=y                       # NFT版本的iptables（现代化防火墙框架）
CONFIG_PACKAGE_ip6tables-nft=y                      # NFT版本的ip6tables（IPv6防火墙）
CONFIG_PACKAGE_firewall4=y                          # 新一代防火墙（原生支持IPv6）
CONFIG_PACKAGE_firewall4-iptables=y                 # 防火墙兼容层（支持IPv6）
CONFIG_PACKAGE_luci-app-firewall=y                  # 防火墙LuCI界面（支持IPv6配置）

# Docker依赖防火墙模块（完整IPv6支持）
CONFIG_PACKAGE_iptables-mod-extra=y                 # 额外的iptables模块（同时支持IPv4/IPv6）
CONFIG_PACKAGE_ip6tables-mod-fullconenat=y          # IPv6全锥形NAT模块（提升部分应用兼容性）
CONFIG_PACKAGE_iptables-mod-fullconenat=y           # IPv4全锥形NAT模块（提升部分应用兼容性）

# mwan3依赖工具（含IPv6）
CONFIG_PACKAGE_iptables-mod-conntrack-extra=y       # 额外的连接跟踪模块（同时支持IPv4/IPv6）

# ===============================================================================
# 12. 默认LuCI应用（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_luci-app-adguardhome=y               # AdGuardHome广告过滤（同时支持IPv4/IPv6过滤）
CONFIG_PACKAGE_luci-app-adguardhome_INCLUDE_binary=n # 不包含AdGuardHome二进制文件（减小固件体积）
CONFIG_PACKAGE_luci-app-autoreboot=y                # 自动重启应用
CONFIG_PACKAGE_luci-app-cupsd=m                     # 打印机服务（模块化）
CONFIG_PACKAGE_luci-app-diskman=y                   # 磁盘管理应用
CONFIG_PACKAGE_luci-app-easymesh=y                  # 简易Mesh网络配置工具
CONFIG_PACKAGE_luci-app-istorex=y                   # 软件商店
CONFIG_PACKAGE_luci-app-lucky=y                     # 网络工具（同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-app-oaf=y                       # 应用过滤（同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-app-quickfile=y                 # 文件快速访问
CONFIG_PACKAGE_luci-app-samba4=y                    # Samba文件共享（同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-app-smartdns=y                  # SmartDNS域名解析（同时支持IPv4/IPv6智能分流）
CONFIG_PACKAGE_luci-app-sqm=y                       # 流量控制（同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-app-turboacc=y                  # 网络加速工具
CONFIG_PACKAGE_luci-app-ttyd=y                      # 网页终端
CONFIG_PACKAGE_luci-app-vlmcsd=y                    # KMS激活服务
CONFIG_PACKAGE_luci-app-wol=y                       # 网络唤醒（同时支持IPv4/IPv6）
CONFIG_PACKAGE_luci-app-package-manager=y           # 包管理应用
CONFIG_PACKAGE_luci-app-athena-led=m                # LED控制应用（模块化）
CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=m         # LED控制应用中文支持（模块化）
CONFIG_PACKAGE_luci-app-ipv6-helper=y               # IPv6配置辅助工具（简化IPv6设置）
CONFIG_PACKAGE_luci-app-argon-config=y              # Argon主题配置工具

# ===============================================================================
# 13. 新增插件（完整IPv6支持）
# ===============================================================================
CONFIG_PACKAGE_luci-app-openclash=y                 # OpenClash代理工具（同时支持IPv4/IPv6代理）
CONFIG_PACKAGE_luci-app-parentcontrol=y             # 时间控制应用（同时支持IPv4/IPv6）

